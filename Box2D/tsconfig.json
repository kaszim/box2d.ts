{
  "compilerOptions": {
    "target": "ES2016",
    "module": "system",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "inlineSourceMap": true,
    "types": [
      "war3-types/core/compat",
      "war3-types/core/common",
      "war3-types/core/blizzard",
      "war3-types/core/commonai",
      "war3-types/core/polyfill",
      "war3-types/special/w3ts"
    ]
  },
  "include": [
    "**/*.ts"
  ]
}
